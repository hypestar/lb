				<!--Javascript source to be include in head section-->

function trailerType ()
{
    // Der kan ikke vælges vejhjælp, indbosum eller
    // ekstraudstyr til trailere.
    if ($(this).val() == "Trailer")
    {
	// Vejhjælp
	$("#LI3ovA").prop("selectedIndex", 1);
	$("#LI3ovA").prop("disabled", true);
	//Indbosum
	$("#LI2msA").prop("selectedIndex", 0);
	$("#LI2msA").prop("disabled", true);
	//Ekstraudstyr
	$("#LI2mqA").prop("selectedIndex", 0);
	$("#LI2mqA").prop("disabled", true);
    }
    else
    {
	// Vejhjælp
	$("#LI3ovA").prop("disabled", false);
	//Indbosum
	$("#LI2msA").prop("disabled", false);
	//Ekstraudstyr
	$("#LI2mqA").prop("disabled", false);
    }
}

function selskabsSpecifikInit ()
{
    var lb = new RegExp("lb.dk");
    var runa = new RegExp("runa.dk");
    var bauta = new RegExp("bauta.dk");
    var fdm = new RegExp("fdmforsikring.dk");

    if (lb.test(document.URL))
    {
	setVejhjaelpLabel("LB");
    }   
    else if (runa.test(document.URL)) 
    {
	setVejhjaelpLabel("Runa");
    }
    else if (bauta.test(document.URL)) 
    {
	setVejhjaelpLabel("Bauta");
    }
    else if (fdm.test(document.URL)) 
    {
	// FDM tilbyder ikke vejhjælp - Vejhjælp skjules. 
	$("#LI3ovA").closest("div.row").hide();
    }
}

function setVejhjaelpLabel(selskabsNavn)
{
    var label = $("#LI3ovA").closest("div.row").find("u");
    $(label).text(selskabsNavn + " vejhjælp");
}

$(document).ready(
    function () 
    {
	selskabsSpecifikInit ();
	// Type
	$("#LI2muA").change(trailerType);
    });


			</src>
